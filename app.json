{
  "expo": {
    "name": "MoodMuse",
    "slug": "MoodMuse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://i.pinimg.com/736x/43/51/dc/4351dc62e75516f5c34c8cfd4eceec75.jpg",
    "scheme": "moodmuse",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.moodmuse",
      "backgroundColor": "#cb42f5",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#cb42f5"
      },
      "package": "com.yourname.moodmuse",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#cb42f5"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true   
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c78333fa-b39b-40c5-a44a-ea36511527ca"
      }
    }
  }
}
